<div class="row-fluid">

  <div class="span4">
    <h1>Home#index</h1>
    <h3>Home</h3>
    <% @users.each do |user| %>
        <p>User: <%=link_to user.UserName, user %></p>
    <% end %>
  </div>

  <div class="span8">
    <h1>Zoom-in to explore cadastral map</h1>
  </div>
</div>

<div class="row-fluid">
  <div class="span5">
    <form class="form-search">
      <input id="address" type="text" placeholder="Type address" class="input-medium search-query">
      <input type="button" class="btn btn-primary" value="Search for a location" onclick="codeAddress()">
    </form>
  </div>

  <div class="span2">
    <span class="title">Overlay opacity</span>
  </div>

  <div class="span5">
    <div id="slider"></div>
  </div>
</div>

<div class="row-fluid">
  <div class="span5">
    <button class="btn btn-primary" onclick="getLocation()"><i class="icon-screenshot icon-white"></i>Show current location</button>
  </div>
</div>

<div id="map-canvas"></div>